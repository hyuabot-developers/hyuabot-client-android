<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@android:color/background_light">
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/app_bar_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">
        <com.google.android.material.appbar.MaterialToolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/hanyang_blue"
            app:menu="@menu/full_screen_menu"
            app:titleCentered="true"
            app:titleTextColor="@android:color/white" />
    </com.google.android.material.appbar.AppBarLayout>
    <com.kakao.vectormap.MapView
        android:id="@+id/stop_map_view"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        app:layout_constraintTop_toBottomOf="@id/app_bar_layout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
    <TextView
        android:id="@+id/shuttle_first_last_time_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="16dp"
        android:text="@string/shuttle_first_last_time_title"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textColor="@android:color/primary_text_light"
        android:textFontWeight="600"
        app:layout_constraintTop_toBottomOf="@id/stop_map_view" />
    <LinearLayout
        android:id="@+id/shuttle_first_last_time_header_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@id/shuttle_first_last_time_title">
        <TextView
            android:id="@+id/shuttle_first_last_time_type"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/shuttle_first_last_time_type"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textColor="@android:color/primary_text_light"
            android:textFontWeight="600" />
        <TextView
            android:id="@+id/shuttle_first_last_time_first"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/shuttle_first_last_time_first"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textColor="@android:color/primary_text_light"
            android:textFontWeight="600" />
        <TextView
            android:id="@+id/shuttle_first_last_time_last"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/shuttle_first_last_time_last"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textColor="@android:color/primary_text_light"
            android:textFontWeight="600" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/shuttle_first_last_time_station_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@id/shuttle_first_last_time_header_layout">
        <TextView
            android:id="@+id/shuttle_for_station_type"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/shuttle_bound_for_station"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:gravity="center"
            android:textSize="18sp"
            android:textColor="@android:color/primary_text_light"
            android:textFontWeight="600" />
        <LinearLayout
            android:id="@+id/shuttle_for_station_first"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_station_first_time_weekdays"
                android:text="@string/shuttle_first_last_time_no_data_weekdays"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_station_first_time_weekends"
                android:text="@string/shuttle_first_last_time_no_data_weekends"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/shuttle_for_station_last"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_station_last_time_weekdays"
                android:text="@string/shuttle_first_last_time_no_data_weekdays"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_station_last_time_weekends"
                android:text="@string/shuttle_first_last_time_no_data_weekends"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/shuttle_first_last_time_dormitory_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@id/shuttle_first_last_time_station_layout">
        <TextView
            android:id="@+id/shuttle_for_dormitory_type"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/shuttle_bound_for_dormitory"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:gravity="center"
            android:textSize="18sp"
            android:textColor="@android:color/primary_text_light"
            android:textFontWeight="600" />
        <LinearLayout
            android:id="@+id/shuttle_for_dormitory_first"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_dormitory_first_time_weekdays"
                android:text="@string/shuttle_first_last_time_no_data_weekdays"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_dormitory_first_time_weekends"
                android:text="@string/shuttle_first_last_time_no_data_weekends"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/shuttle_for_dormitory_last"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_dormitory_last_time_weekdays"
                android:text="@string/shuttle_first_last_time_no_data_weekdays"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_dormitory_last_time_weekends"
                android:text="@string/shuttle_first_last_time_no_data_weekends"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/shuttle_first_last_time_terminal_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@id/shuttle_first_last_time_dormitory_layout">
        <TextView
            android:id="@+id/shuttle_for_terminal_type"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/shuttle_bound_for_terminal"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:gravity="center"
            android:textSize="18sp"
            android:textColor="@android:color/primary_text_light"
            android:textFontWeight="600" />
        <LinearLayout
            android:id="@+id/shuttle_for_terminal_first"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_terminal_first_time_weekdays"
                android:text="@string/shuttle_first_last_time_no_data_weekdays"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_terminal_first_time_weekends"
                android:text="@string/shuttle_first_last_time_no_data_weekends"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/shuttle_for_terminal_last"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_terminal_last_time_weekdays"
                android:text="@string/shuttle_first_last_time_no_data_weekdays"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_terminal_last_time_weekends"
                android:text="@string/shuttle_first_last_time_no_data_weekends"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/shuttle_first_last_time_jungang_station_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@id/shuttle_first_last_time_terminal_layout">
        <TextView
            android:id="@+id/shuttle_for_jungang_station_type"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/shuttle_bound_for_jungang_station"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:gravity="center"
            android:textSize="18sp"
            android:textColor="@android:color/primary_text_light"
            android:textFontWeight="600" />
        <LinearLayout
            android:id="@+id/shuttle_for_jungang_station_first"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_jungang_station_first_time_weekdays"
                android:text="@string/shuttle_first_last_time_no_data_weekdays"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_jungang_station_first_time_weekends"
                android:text="@string/shuttle_first_last_time_no_data_weekends"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/shuttle_for_jungang_station_last"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_jungang_station_last_time_weekdays"
                android:text="@string/shuttle_first_last_time_no_data_weekdays"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="8dp"
                android:id="@+id/shuttle_for_jungang_station_last_time_weekends"
                android:text="@string/shuttle_first_last_time_no_data_weekends"
                android:textAlignment="center"
                android:textSize="18sp"
                android:textColor="@android:color/primary_text_light" />
        </LinearLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/shuttle_first_last_time_jungang_station_layout" />
</androidx.constraintlayout.widget.ConstraintLayout>
